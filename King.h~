#ifndef KING_H
#define KING_H

#include <string>
#include "Card.h"


namespace Solitaire
{
  class King : public Card {

  public:

    char to_ascii() const override { return "K"; }

    char get_lower() const override { return "?"; }

    char get_upper() const override { return "Q"; }
  };
}
#endif
